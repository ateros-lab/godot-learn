[sub_resource type="GDScript" id="GDScript_pmwcq"]
script/source = "extends Node

@onready var planet: Node3D = $\"../Planet\"
@onready var label: Label = $\"../UserInterface/Label\"
var planet_count: int = 5
const PLANET: Resource = preload(\"res://scenes/planet.tscn\")
const LABEL_TEXT: String = 'Planet count - '

# Called when the node enters the scene tree for the first time.
func _ready() -> void:
	for i in range(1, 5):
		var planet_instance = PLANET.instantiate()
		planet_instance.position = planet.position
		planet_instance.position.z -= i
		add_child(planet_instance)


# Called every frame. 'delta' is the elapsed time since the previous frame.
func _process(delta: float) -> void:
	pass


func _on_player_area_area_entered(area: Area3D) -> void:
	if area.is_in_group('planet'):
		if area.planet.visible:
			area.hide_planet()
			planet_count -= 1
			label.text = LABEL_TEXT + str(planet_count) # int() float() bool()
"

